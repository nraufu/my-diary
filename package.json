{
  "name": "my-diary",
  "version": "1.0.0",
  "description": "MyDiary is an online journal where users can pen down their thoughts and feelings.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && node dist/index.js --exec babel-node",
    "build": "babel server -d dist",
    "start": "npm run build && node dist/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --require @babel/register ./server/test/*.spec.js --timeout 10000 --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nraufu/my-diary.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nraufu/my-diary/issues"
  },
  "homepage": "https://github.com/nraufu/my-diary#readme",
  "dependencies": {
    "@babel/register": "^7.6.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "nyc": "^14.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.6",
    "config": "^3.2.4",
    "coveralls": "^3.0.7",
    "dotenv": "^8.2.0",
    "eslint": "^6.5.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "pg": "^7.12.1",
    "pg-connection-string": "^2.1.0"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
